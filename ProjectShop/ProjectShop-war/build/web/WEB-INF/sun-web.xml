<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD GlassFish Application Server 3.0 Servlet 3.0//EN" "http://www.sun.com/software/appserver/dtds/sun-web-app_3_0-0.dtd">
<sun-web-app error-url="">
  <context-root>/ProjectShop-war</context-root>
  <servlet>
    <servlet-name>ExecServlet</servlet-name>
    <servlet-mapping>
      <servlet-name>ExecServlet</servlet-name>
      <url-pattern>/registration.html</url-pattern>
    </servlet-mapping>
    <principal-name class-name="servlets.ExecServlet">ExecServlet</principal-name>
  </servlet>
  <servlet>
    <servlet-name>AddImageServlet</servlet-name>
    <principal-name class-name="servlets.AddImageServlet">AddImageServlet</principal-name>
  </servlet>
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="keepgenerated" value="true">
      <description>Keep a copy of the generated servlet class' java code.</description>
    </property>
  </jsp-config>
</sun-web-app>
